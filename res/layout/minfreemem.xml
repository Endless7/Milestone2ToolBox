<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="wrap_content">

	<TextView android:text="@string/mfm_tv_mem1" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<EditText android:id="@+id/etMem1" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<TextView android:text="@string/mfm_tv_mem2" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<EditText android:id="@+id/etMem2" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<TextView android:text="@string/mfm_tv_mem3" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<EditText android:id="@+id/etMem3" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<TextView android:text="@string/mfm_tv_mem4" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<EditText android:id="@+id/etMem4" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<TextView android:text="@string/mfm_tv_mem5" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<EditText android:id="@+id/etMem5" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<TextView android:text="@string/mfm_tv_mem6" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<EditText android:id="@+id/etMem6" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="horizontal" android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<CheckBox android:id="@+id/cbMemAutoApply" android:text="@string/mfm_cbMemAutoApply"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<Button android:id="@+id/btnMemSaveDefault" android:text="@string/mfm_btnMemSaveDefault"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
	</LinearLayout>
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="horizontal" android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<Button android:id="@+id/btnMemDefault" android:text="@string/mfm_btnMemDefault"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<Button android:id="@+id/btnMemMild" android:text="@string/mfm_btnMemMild"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<Button android:id="@+id/btnMemPositive" android:text="@string/mfm_btnMemPositive"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<Button android:id="@+id/btnMemDefined" android:text="@string/mfm_btnMemDefined"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<Button android:id="@+id/btnMemApply" android:text="@string/mfm_btnMemApply"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
	</LinearLayout>
</LinearLayout>